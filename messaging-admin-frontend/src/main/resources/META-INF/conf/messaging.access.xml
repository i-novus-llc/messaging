<access xmlns="http://n2oapp.net/framework/config/schema/access-2.0">
    <role id="admin">
        <url-access pattern="${messaging.user.admin.url}"/>
    </role>

    <permit-all>
        <object-access object-id="messaging_feed" operations="*"/>
        <object-access object-id="messaging_message" operations="*"/>
        <object-access object-id="messaging_user" operations="*"/>
        <object-access object-id="messaging_msg_setting" operations="*"/>
    </permit-all>

</access>
