<?xml version='1.0' encoding='UTF-8'?>
<simple-page xmlns="http://n2oapp.net/framework/config/schema/page-4.0"
             name="Профиль пользователя">
    <form>
        <fields>
            <set enabled="false">
                <input-text id="username" label="Логин" default-value="#{username}"/>
                <input-text id="surname" label="Фамилия" default-value="#{surname}"/>
                <input-text id="name" label="Имя" default-value="#{name}"/>
                <input-text id="patronymic" label="Отчество" default-value="#{patronymic}"/>
                <input-text id="email" label="Электронная почта" default-value="#{email}"/>
            </set>
        </fields>
    </form>
</simple-page>
