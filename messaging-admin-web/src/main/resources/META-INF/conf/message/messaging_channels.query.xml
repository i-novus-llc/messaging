<?xml version='1.0' encoding='UTF-8'?>
<query xmlns="http://n2oapp.net/framework/config/schema/query-4.0">
    <list>
        <rest method="GET">${messaging.backend.path}/${messaging.tenant-code}/channels</rest>
    </list>

    <fields>
        <field id="id" domain="string">
            <select/>
            <filters>
                <eq filter-id="id"/>
            </filters>
        </field>

        <field id="name">
            <select/>
            <filters>
                <like filter-id="name"/>
            </filters>
        </field>
    </fields>
</query>
